        {
            "name": "Digital Health Interventions",
            "description": "Various digital health interventions for mental health treatment.",
            "guidelines": [
                {
                    "level": "Guided",
                    "drugs": [
                        {
                            "name": "Good Days Ahead",
                            "RCT": "Yes",
                            "notes": "",
                            "description": "Program designed for use by a CBT therapist to provide additional CBT instruction outside of the session.",
                            "comments": "Two large RCTs showed this program reduced the need for face-to-face CBT by half, with equivalent clinical outcomes."
                        },
                        {
                            "name": "BounceBack",
                            "RCT": "",
                            "notes": "",
                            "description": "Free online CBT program with various formats: self-directed version, or with telephone-delivered lay coaching.",
                            "comments": "No RCT but a large prospective study was positive in MDD; available free in several Canadian provinces, managed by the Canadian Mental Health Association."
                        },
                        {
                            "name": "Deprexis",
                            "RCT": "Yes",
                            "notes": "Not Available in Canada",
                            "description": "Online CBT program with tailored guidance provided by an AI conversational agent; includes BA and mindfulness techniques.",
                            "comments": "Structured 12-week program designed for 90-day use; available in many countries in several languages; tested in multiple clinical trials, with a positive meta-analysis. Available in Europe and the USA."
                        },
                        {
                            "name": "MoodBeacon",
                            "RCT": "",
                            "notes": "",
                            "description": "Online guided CBT program; guidance provided by trained therapists.",
                            "comments": "Although there is no published study, this DHI was reviewed and approved for use by the Province of Ontario in 2021."
                        },
                        {
                            "name": "Pacifica/Sanvello",
                            "RCT": "Yes",
                            "notes": "Not Available in Canada",
                            "description": "Mobile- and web-based apps intended to assist users in relieving stress, anxiety, and depression. Free; offers in-app purchases.",
                            "comments": "Uses blend of CBT, mindfulness, and relaxation tools; suitable for adults and adolescents; guidance is through peer discussion in chat groups; has positive results from an RCT. Available in the USA."
                        }
                    ]
                },
                {
                    "level": "Unguided",
                    "drugs": [
                        {
                            "name": "Catch It",
                            "RCT": "",
                            "notes": "",
                            "description": "Free CBT-based mobile app, primarily uses automatic thought records and reflection exercises to address anxiety and depressive symptoms.",
                            "comments": "Developed jointly by the Universities of Liverpool and Manchester; includes a mood tracker and diary; and has an observational trial of users showing benefits."
                        },
                        {
                            "name": "Headspace",
                            "RCT": "",
                            "notes": "",
                            "description": "Popular mobile app with self-guided meditation and mindfulness techniques and some CBT approaches.",
                            "comments": "Designed for a wide variety of stress, depression and anxiety symptoms; extensively studied with good outcomes in many conditions (of mild severity), but not specifically in MDD."
                        },
                        {
                            "name": "MoodGYM",
                            "RCT": "Yes",
                            "notes": "",
                            "description": "Internet-based CBT self-help program, with very structured modules.",
                            "comments": "Most widely used platform; initially developed by Australian National University; extensively studied with good outcomes in mild depression."
                        },
                        {
                            "name": "MoodKit",
                            "RCT": "Yes",
                            "notes": "Not Available in Canada",
                            "description": "CBT-based toolkit app with detailed instructions on how to use thought records and BA.",
                            "comments": "Teaches multiple aspects of CBT and is well-integrated with mood monitoring and optional journaling. Available in the USA."
                        },
                        {
                            "name": "Spark Direct",
                            "RCT": "Yes",
                            "notes": "Not Available in Canada",
                            "description": "A mobile app providing core elements of CBT and BA in a 5-week program.",
                            "comments": "Designed for youth depression; uses interactive tasks and rewards; tested in an RCT with positive findings. Available in the USA."
                        }
                    ]
